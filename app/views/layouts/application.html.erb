<!DOCTYPE html>
<html>
  <head>
    <title>Packsy - Travel Items, Checked</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag asset_path('favicon.ico'), :rel => 'icon', :type => "image/x-icon" %>
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
    <script src="https://use.fontawesome.com/94097fa447.js"></script>
    <script type="text/javascript">
      <% if logged_in? %>
        window.currentUser =
          <%= render("api/users/user.json.jbuilder", user: current_user).html_safe %>
      <% end %>
    </script>
  </head>


  <body>
    <%= yield %>
  </body>
</html>
